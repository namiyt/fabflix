<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>FabFlix</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Auto</servlet-name>
    <servlet-class>com.AutoComplete.sample.AutoComplete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Auto</servlet-name>
    <url-pattern>/Auto</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PopupMovie</servlet-name>
    <servlet-class>servlet.PopupMovie</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PopupMovie</servlet-name>
    <url-pattern>/PopupMovie</url-pattern>
  </servlet-mapping>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>HTTPSOnly</web-resource-name>
      <url-pattern>/*</url-pattern>
    </web-resource-collection>
    <user-data-constraint>
      <transport-guarantee>CONFIDENTIAL</transport-guarantee>
    </user-data-constraint>
  </security-constraint>
</web-app>